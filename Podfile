source 'https://github.com/redspark-io/redspark-ios-specs.git'
source 'https://github.com/CocoaPods/Specs.git'

platform :ios, '9.0'
use_frameworks!
inhibit_all_warnings!

target 'gestta' do
    
    # Internal libs
    pod 'RSStarterKit'
    pod 'RSWidgets'
    
    # Android TabBar
    pod 'ScrollPager', '~> 0.9'

    target 'gesttaTests' do
        inherit! :search_paths
    end

    target 'gesttaUITests' do
        inherit! :search_paths
    end

end
